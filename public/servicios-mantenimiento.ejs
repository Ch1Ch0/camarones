<script type="text/javascript">

	 $(document).ready(function(){
		// Initialize the gallery
		// $('.magnifier2').touchTouch();
			
		var obj = { page: "Servicios" };
		cargar(obj);
		
		var foot = { page: "Contactar" };
		cargar(foot);
		
	 });

	function cargar(obj){
		var json_text = JSON.stringify(obj, null, 2);

		$.ajax({
			url: '/api/user/getcmscontent',
			type: 'POST',
			contentType: 'application/json',
			data: json_text,
			success: function (response) {
				 // console.log("respuesta: " + response);
				if (response.message == "Records loaded successfully"){
					for (var i = 0; i < response.elements.length; i++) {
						switch(response.elements[i].campo){
							// CASOS ESPECIALES, QUE OCUPAN UN SPAN BOLD
							case "Mantenimiento_titulo_3_subtitulo_1":
							case "Mantenimiento_titulo_3_subtitulo_2": {
								var txt2 = response.elements[i].valor.search(":") !== -1 ? "<span class='bold'>" + response.elements[i].valor.replace(':', ':</span> ') : response.elements[i].valor;
								$("."+response.elements[i].campo).append(txt2);
								break;
							}
							// DIRECCIONES URL QUE NECESITAN SER INYECTADAS EN HREF.
							case "direccion_facebook": {
								$("."+response.elements[i].campo).attr('href', response.elements[i].valor);
								break;
							}
							case "correo": {
								$("."+response.elements[i].campo).attr('href', 'mailto:' + response.elements[i].valor);
								$("."+response.elements[i].campo).text(response.elements[i].valor);
								break;
							}
							case "telefono": {
								$("."+response.elements[i].campo).attr('href', 'tel:' + response.elements[i].valor);
								$("."+response.elements[i].campo).text(response.elements[i].valor);
								break;
							}
							
							// CASOS ESPECIALES, llevan ícono por lo que se requiere hacer un append.
							case "Mantenimiento_titulo_4_item_1": case "Mantenimiento_titulo_4_item_2":
							case "Mantenimiento_titulo_4_item_3": case "Mantenimiento_titulo_4_item_4": 
							case "Mantenimiento_titulo_4_item_5": case "Mantenimiento_titulo_4_item_6": 
							case "Mantenimiento_titulo_4_item_7": case "Mantenimiento_titulo_4_item_8":
							case "Mantenimiento_titulo_4_item_9": case "Mantenimiento_titulo_4_item_10": 
							case "Mantenimiento_titulo_4_item_11": {
								$("."+response.elements[i].campo).append(response.elements[i].valor);
								break;
							}
							// TEXTO RESTANTE && NORMAL
							default: {
								$("."+response.elements[i].campo).text(response.elements[i].valor);
								break;
							}
						}
					}
				}
			}
		});
	}
</script>

